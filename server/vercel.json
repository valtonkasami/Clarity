{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/assets/(.*)",
        "headers": { "Access-Control-Allow-Origin": "*" },
        "methods": ["POST"],
        "continue": true,
        "dest": "/index.js"
      },
      {
        "src": "/public/assets/(.*)",
        "headers": { "Access-Control-Allow-Origin": "*" },
        "methods": ["POST"],
        "continue": true,
        "dest": "/index.js"
      },
      {
        "src": "/auth/register",
        "methods": ["POST"],
        "dest": "/index.js",
        "headers": { "Access-Control-Allow-Origin": "*" },
        "continue": true
      },
      {
        "src": "/posts/create",
        "methods": ["POST"],
        "dest": "/index.js",
        "continue": true,
        "headers": { "Access-Control-Allow-Origin": "*" }
      },
      {
        "src": "/users/create",
        "methods": ["POST"],
        "dest": "/index.js",
        "headers": { "Access-Control-Allow-Origin": "*" },
        "continue": true
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }