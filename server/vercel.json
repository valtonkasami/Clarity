{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/assets/(.*)",
        "headers": { "Access-Control-Allow-Origin": "*" },
        "continue": true
      },
      {
        "src": "/assets/(.*)",
        "dest": "/public/assets/$1"
      },
      {
        "src": "/api/upload",
        "methods": ["POST"],
        "dest": "/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }